<?xml version="1.0"?>
<robot name="evarobot">

<!-- Base -->
  <link name="base_link">
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	<box size="0.30 0.251 0.3005"/>
      </geometry>
    </collision>

    <visual>
     <origin xyz="0.05 0 -0.1475" rpy="0 0 1.5708" /> 
      <geometry>
        <mesh filename="package://evarobot_description/meshes/evarobot_all.stl" scale="0.001 0.001 0.001"/> 
      </geometry>
      <material name="blue">
        <color rgba="0 0 .8 1"/>
      </material>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia
        ixx="5.0" ixy="0.0" ixz="0.0"
        iyy="5.0" iyz="0.0"
        izz="5.0"/>
    </inertial>
  </link>

<!-- Left Wheel -->
    
   <link name="left_front_wheel">
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length=".024" radius="0.0675"/>
      </geometry>
    </collision>
    
<!--<visual>
	<origin rpy="0 -1.57075 0" xyz="0 0 +0.01"/>
